var temp = $(document).height();

$("#submissions-div").append("<%= j render partial: 'submissions' %>");
$("#load_more_link").replaceWith("<%= j render partial: 'shared/load_more', locals: { collection: @submissions }, :id=>'load_more_link' %>");

$('html, body').animate({
   scrollTop: 2*temp - $(document).height()},
   1400,
   "easeOutQuint"
);
