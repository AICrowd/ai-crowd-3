$(".load-more").remove();
$(".table-leaderboard-body").append("<%= j render partial: 'rating_leaderboard', locals: { participants: @participants } %>");
<% if @participants.present? %>
  $("#more-anchor").after("<%= j render partial: 'shared/load_more', locals: { collection: @participants } %>");
<% end %>

$('html, body').animate({
        scrollTop: $(document).height()-$(window).height()},
    1400,
    "easeOutQuint"
);
