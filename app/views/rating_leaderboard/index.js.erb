$(".load-more").remove();
$(".table-leaderboard").append("<%= j render partial: 'rating_leaderboard', locals: { participants: @participants } %>");
$("#more-anchor").after("<%= j render partial: 'shared/load_more', locals: { collection: @participants } %>");
$('html, body').animate({
        scrollTop: $(document).height()-$(window).height()},
    1400,
    "easeOutQuint"
);
