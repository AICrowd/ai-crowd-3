<% if @participants.count < 10 %>
  $('.table-leaderboard-body').attr("data-request-completed", false);
<% else %>
  $('.table-leaderboard-body').attr("data-request-completed", true);
<% end %>
$("#loader-container").hide();
$(".table-leaderboard-body").append("<%= j render partial: 'rating_leaderboard', locals: { participants: @participants } %>");
