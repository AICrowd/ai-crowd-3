$(".load-more").remove();
$("#challenges-div").append("<%= j render partial: 'challenges' %>");
$("#more-anchor").after("<%= j render partial: 'shared/load_more', locals: { collection: @challenges } %>");
$('html, body').animate({
        scrollTop: $(document).height()-$(window).height()},
    1400,
    "easeOutQuint"
);
