<% if @discourse_topics.size < 30 %>
  $('#discourse-topics-listing').attr("data-request-completed", false);
<% else %>
  $('#discourse-topics-listing').attr("data-request-completed", true);
<% end %>
$("#loader-container").hide();
$("#discourse-topics-listing").append("<%= j render partial: 'shared/discourse/discussions_list', locals: { discourse_topics: @discourse_topics } %>");
